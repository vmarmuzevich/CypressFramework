{
  "name": "cypressframework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "cypress run --spec ./cypress/integration/test/specs/*.js --browser chrome",
    "cy:run": "cypress run --browser chrome --headless",
    "test-allure": "cypress run --browser chrome --headless --reporter mocha-allure-reporter",
    "allure": "allure generate --clean allure-results && allure open",
    "finaltest": "npm run test-allure && npm run allure",
    "run": "./node_modules/.bin/cypress run --browser chrome",
    "allure open": "allure open"
  },
  "author": "iDocket",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.1.0",
    "@cucumber/cucumber": "^7.3.2",
    "allure-commandline": "^2.17.2",
    "cypress": "^9.5.0",
    "cypress-downloadfile": "^1.2.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^1.6.2",
    "mocha-allure-reporter": "^1.4.0",
    "cucumber-html-reporter": "^5.5.0",
    "multiple-cucumber-html-reporter": "^1.18.3"
  },
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.26.5",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "chai": "^4.3.6",
    "typescript": "^4.5.5",
    "prettier": "^2.5.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
